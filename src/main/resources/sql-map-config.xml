<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

    <!-- Alias 설정 -->
    <typeAliases>
        <typeAlias alias="user" type="com.moviment.model.UserVO" />
        <typeAlias alias="movie" type="com.moviment.model.MovieVO" />
    </typeAliases>

    <!-- Sql Mapper 설정 -->
    <mappers>
        <mapper resource="mappings/user-mapping.xml" />
        <mapper resource="mappings/movie-mapping.xml" />
    </mappers>

</configuration>